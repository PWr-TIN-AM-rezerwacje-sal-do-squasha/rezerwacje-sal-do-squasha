<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.userName %>
  </p>

  <p>
    <strong>Usersurname:</strong>
    <%= user.userSurname %>
  </p>

  <p>
    <strong>Userpassword:</strong>
    <%= user.userPassword %>
  </p>

  <p>
    <strong>Useremail:</strong>
    <%= user.userEmail %>
  </p>

  <p>
    <strong>Userbirthdate:</strong>
    <%= user.userBirthDate %>
  </p>

  <p>
    <strong>Userphonenumber:</strong>
    <%= user.userPhoneNumber %>
  </p>

</div>
